{
    "name": "zeuxisoo/slim-simplework",
    "description": "PHP Slim Framework skeleton application",
    "homepage": "https://github.com/zeuxisoo/php-slim-simplework",
    "require": {
        "slim/slim": "^3.3",
        "slim/twig-view": "^2.1",
        "monolog/monolog": "^1.18",
        "symfony/var-dumper": "^3.0",
        "illuminate/database": "^5.2",
        "Illuminate/Pagination": "^5.2",
        "robmorgan/phinx": "^0.5.3",
        "slim/flash": "^0.1.0",
        "slim/csrf": "^0.6.0",
        "respect/validation": "^1.0",
        "league/fractal": "^0.13.0",
        "firebase/php-jwt": "^3.0",
        "nesbot/carbon": "^1.21",
        "symfony/translation": "^3.0",
        "symfony/twig-bridge": "^3.0",
        "maximebf/debugbar": "^1.11",
        "illuminate/events": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
